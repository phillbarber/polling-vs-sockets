          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion" enabled="false">
            <stringProp name="BeanShellAssertion.query">Integer pollingDelayMS = Integer.parseInt(vars.get(&quot;pollingDelayMs&quot;));
Integer pollingCount = Integer.parseInt(vars.get(&quot;pollingCount&quot;));

Integer maxTime = Integer.parseInt(vars.get(&quot;maxDurationOfJobToComplete&quot;));

Integer currentTime = pollingCount * pollingDelayMS;

if (currentTime &gt; maxTime ){
	Failure = true;
	SampleResult.setSuccessful(false);
	FailureMessage = &quot;MaxTime: &quot; + maxTime + &quot; Took: &quot; + currentTime + &quot; and job still not complete&quot;;
}
</stringProp>
            <stringProp name="BeanShellAssertion.filename"></stringProp>
