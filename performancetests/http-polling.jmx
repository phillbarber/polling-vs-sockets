          <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion" enabled="true">
            <stringProp name="BeanShellAssertion.query">
long startTime = Long.parseLong(vars.get(&quot;startTime&quot;));
long currentTime = System.currentTimeMillis();
long maxDurationOfJobToComplete = Long.parseLong(vars.get(&quot;maxDurationOfJobToComplete&quot;));

if ( currentTime &gt; startTime + maxDurationOfJobToComplete ){
	Failure = true;
	FailureMessage = &quot;Job: &quot; + vars.get(&quot;jobID&quot;) + &quot; was too slow! Start Time: &quot; + startTime + &quot; CurrentTime: &quot; + currentTime + &quot; MaxDurationOfJobToComplete: &quot; + maxDurationOfJobToComplete;
}
</stringProp>
            <stringProp name="BeanShellAssertion.filename"></stringProp>
